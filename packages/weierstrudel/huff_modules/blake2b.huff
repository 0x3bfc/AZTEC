/**
 * @title Blake2b
 *
 * @author Zachary Williamson
 * A Huff smart contract that performs the Blake2b hashing algorithm
 * Costs ~16,130 gas per compression round, plus ~630 gas in setup costs
 *
 * Cost to hash 128 bytes ~14,503 gas
 * Cost to hash 5kb data ~551,654 gas
 * Cost of 512 compression rounds (equihash) ~7,061,173 gas
 * 
 **/

/** 
 * V arrays
 * We store 2 V entries in a single EVM word
 * Each entry is separated by 8 bytes of zero padding,
 * The most and least significant 4 bytes of the word are also zero padded.
 * This allows us to perform bit rotations and uint64 additions with a
 * minimal amount of masking, and we can cut the # of mixing steps in half per compression
 **/
#define macro V_0_1 = takes(0) returns(1)
{
0x000000006a09e667f3bcc9080000000000000000bb67ae8584caa73b00000000
}
#define macro V_2_3 = takes(0) returns(1)
{
0x000000003c6ef372fe94f82b0000000000000000a54ff53a5f1d36f100000000
}
#define macro V_4_5 = takes(0) returns(1)
{
0x00000000510e527fade682d100000000000000009b05688c2b3e6c1f00000000
}
#define macro V_6_7 = takes(0) returns(1)
{
0x000000001f83d9abfb41bd6b00000000000000005be0cd19137e217900000000
}

