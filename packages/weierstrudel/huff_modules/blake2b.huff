#define macro ROR_63_MULTIPLICAND = takes(0) returns(1) {
    0x2000000000000000000000000000000000000000000000002
}
#define macro ROR_32_MULTIPLICAND = takes(0) returns(1) {
    0x100000000000000000000000000000000000000000000000100000000
}
#define macro ROR_24_MULTIPLICAND = takes(0) returns(1) {
